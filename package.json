{
    "name": "feral",
    "displayName": "Feral",
    "description": "Language syntax support for Feral",
    "version": "0.2.1",
    "publisher": "electrux",
    "repository": {
        "type": "git",
        "url": "https://github.com/Feral-Lang/Feral-VSCode.git"
    },
    "engines": {
        "vscode": "^1.76.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "feral",
                "aliases": [
                    "Feral",
                    "feral"
                ],
                "extensions": [
                    ".fer",
                    ".fecl"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "feral",
                "scopeName": "source.fer",
                "path": "./syntaxes/feral.tmLanguage.json"
            }
        ]
    }
}
